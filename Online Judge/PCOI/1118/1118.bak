var s:array[1..10000] of ansistring;
    i,j,k:longint;
begin
     s[0]:=' ';
     while (s[i][1]<>'#') do
     begin
          inc(i);
          readln(s[i]);
     end;
     for j:= 1 to i-1 do
     begin
           for k:= 1 to length(s[j]) do
           begin
                if (s[j][k]<>' ') and (s[j][k]<>'!') and (s[j][k]<>'$') and (s[j][k]<>'%') and (s[j][k]<>'(') and (s[j][k]<>')') and (s[j][k]<>'*') then
                write(s[j][k]);
           end;
           writeln;
     end;
end.