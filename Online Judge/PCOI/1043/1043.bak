var y,m,d:array[1..2] of longint;
    c:array[1..12] of char;
    i,j,s,ans:longint;
begin
    for j:= 1 to 2 do
    begin
     s:=0;
     while s<2 do
     begin
     inc(i);
     read(c[i]);
     if c[i]='/' then
     begin
     inc(s);
     dec(i);
     end;
     if s=0 then
     begin
     if i mod 2 =1 then
     d[j]:=(ord(c[i])-ord('0'))*10
     else
     d[j]:=d[j]+(ord(c[i])-ord('0'));
     end;
     if s=1 then
     begin
      if i mod 2 = 1 then
      m[j]:=(ord(c[i])-ord('0'))*10
      else
      m[j]:=m[j]+ord(c[i])-ord('0');
     end;
     end;
     readln(y[j]);
    end;
    if (y[2]>=y[1]) and (m[2]>=m[1]) and (d[2]>=d[1]) then
    begin
    while (d[1]<>d[2]) or (m[1]<>m[2]) or (y[1]<>y[2]) do
    begin
    if (m[1]=1) or (m[1]=3) or (m[1]=5) or (m[1]=7) or (m[1]=8) or (m[1]=10) or (m[1]=12) then
    begin
     inc(ans);
     inc(d[1]);
     if d[1]=31 then
     begin
     d[1]:=0;
     inc(m[1]);
     end;
     if m[1]=13 then
     begin
     m[1]:=1;
     inc(y[1]);
     end;
    end;
    if (m[1]=4) or (m[1]=6) or (m[1]=9) or (m[1]=11) then
    begin
     inc(ans);
     inc(d[1]);
     if d[1]=30 then
     begin
     d[1]:=0;
     inc(m[1]);
     end;
    end;
    if m[1]=2 then
    begin
    if (y[1] mod 400 <> 0) and (y[1] mod 4 = 0) then
    begin
     inc(ans);
     inc(d[1]);
     if d[1]=29 then
     begin
     d[1]:=0;
     inc(m[1]);
     end;
    end
    else
    begin
     inc(ans);
     inc(d[1]);
     if d[1]=28 then
     begin
      d[1]:=0;
      inc(m[1]);
     end;
    end;
    end;
    end;
    writeln(ans+1);
    end
    else
    writeln('input error');
end.
